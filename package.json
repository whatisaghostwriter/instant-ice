{
  "name": "tictactoe-colyseus",
  "version": "1.0.0",
  "description": "HTML5 Tic-Tac-Toe Game powered by Colyseus",
  "scripts": {
    "start": "npx concurrently \"npm:client\" \"npm:server\"",
    "server": "npx ts-node ./server/index.ts --ignore public",
    "client": "npx webpack serve --config webpack.config.js"
  },
  "author": "Jonathan Carrera",
  "dependencies": {
    "@gamestdio/timer": "^1.3.2",
    "colyseus": "^0.14.7",
    "colyseus.js": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pixi.js": "^6.0.0",
    "tweener": "^0.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.1",
    "html-webpack-plugin": "^5.3.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.17.3",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
